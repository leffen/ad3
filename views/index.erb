<!DOCTYPE html>
<html ng-app="ad3">
<head>
  <title>Angular & D3</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link charset="utf-8" href='components/bootstrap/dist/css/bootstrap.min.css' rel="stylesheet" type="text/css">
  <link charset="utf-8" href='components/nvd3/nv.d3.css' rel="stylesheet" type="text/css">
  <link charset="utf-8" href='ad3.css' rel="stylesheet" type="text/css">

</head>
<body>
<div class="jumbotron">
  <h1>Angular & D3</h1>
</div>

<div class="container" ng-controller="MainController">

  <div class="well"><h1>{{info.title}}</h1>

    <div class="row chart">
      <nvd3-line-chart
      data="info.data"
      id="r2_graph"
      showXAxis="true"
      showYAxis="true"
      tooltips="true"
      xAxisTickFormat="xAxisTickFormatFunction()"
      yAxisTickFormat="yAxisTickFormatFunction()"
      interactive="true"
      color="infoColor()"
      >
      </nvd3-line-chart>

    </div>
  </div>
</div>

<script charset="utf-8" src='components/jquery/dist/jquery.js' type="text/javascript"></script>
<script charset="utf-8" src='components/bootstrap/dist/js/bootstrap.min.js' type="text/javascript"></script>
<script charset="utf-8" src='components/angular/angular.js' type="text/javascript"></script>
<script charset="utf-8" src='components/angular-resource/angular-resource.js' type="text/javascript"></script>
<script charset="utf-8" src='components/d3/d3.js' type="text/javascript"></script>
<script charset="utf-8" src='components/nvd3/nv.d3.js' type="text/javascript"></script>
<script charset="utf-8" src='components/angularjs-nvd3-directives/dist/angularjs-nvd3-directives.js' type="text/javascript"></script>

<script charset="utf-8" src='ad3_utils.js' type="text/javascript"></script>
<script charset="utf-8" src='ad3.js' type="text/javascript"></script>

</body>
</html>

